{
  "env": { "browser": true, "es2021": true },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:prettier/recommended"
  ],
  "parserOptions": { "ecmaVersion": 12, "sourceType": "module" },
  "plugins": ["react", "react-hooks", "prettier"],
  "rules": { "react/prop-types": "off" }
}